{
  "alias": "slow-zone.now.sh",
  "name": "slow-zone",
  "build": {
    "env": {
      "REACT_APP_API_BASE_URL": "https://api.slow.zone",
      "REACT_APP_SENTRY_DSN": "@sentry-dsn"
    }
  },
  "routes": [
    { "src": "/", "dest": "index.html" },
    { "src": "/stations/?", "dest": "index.html" },
    { "src": "/stations/.*", "dest": "index.html" },
    { "src": "/nearby/?", "dest": "index.html" },
    { "src": "/recent/?", "dest": "index.html" },
    { "src": "/(.+)", "dest": "$1" }
  ],
  "version": 2
}
